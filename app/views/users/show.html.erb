<div class="contents row">
  <p><%= @nickname %>さんの今までの投稿</p>
  <% @articles.each do |article| %>
    <div class="content_post" style="background-image: url(<%= article.image %>);">
      <%= simple_format(article.text) %>
      <span class="name"><%= article.title %></span>
    </div>
  <% end %>
  <%= paginate(@articles) %>
</div>